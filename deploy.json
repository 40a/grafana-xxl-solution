{
	"containers": [{
		"image": "{{.IMAGE}}",
		"name": "{{.NAME}}-{{.HASH}}",
		"limits": {
			"cpu": {{.CPU}},
			"memory": {{.RAM}}
		},
		"env": [{
            	"name": "GF_SECURITY_ADMIN_PASSWORD",
            	"value": "{{.PASSWORD}}"
        	}
		]
	}],
	"name": "{{.NAME}}-{{.HASH}}",
	"replicas": 1
}